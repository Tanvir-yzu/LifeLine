upstream uwsgi_LifeLine {
        server unix:/opt/LifeLine/uwsgi/uwsgi.sock;
}
server {
    listen 80;
    server_name blood.en2bn.com ;
    
    access_log /opt/LifeLine/logs/access.log;
    error_log /opt/LifeLine/logs/error.log;

    charset utf-8;
    client_max_body_size 16M;
    
    location /static/ {
        root /opt/LifeLine;
    }
    
    location /media/ {
        root /opt/LifeLine;
    }
    
    location / {
        uwsgi_pass uwsgi_LifeLine;
        include uwsgi_params;
        uwsgi_read_timeout 300s;
        uwsgi_send_timeout 300s;
    }
    
}